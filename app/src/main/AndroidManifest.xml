<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.svetlana.finalexamsqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/final_Exam"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.svetlana.finalexamsqlite.AccomplishingTaskActivity"
            android:parentActivityName="com.example.svetlana.finalexamsqlite.MainActivity"/>
        <activity
            android:name="com.example.svetlana.finalexamsqlite.TaskActivity"
            android:label="Add new task"
            android:parentActivityName="com.example.svetlana.finalexamsqlite.MainActivity" />
        <activity
            android:name="com.example.svetlana.finalexamsqlite.MainActivity"
            android:label="All My Tasks">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>